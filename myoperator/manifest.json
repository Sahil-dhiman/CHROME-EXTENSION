{
  "manifest_version": 2,

  "name": "MY OPERATOR",
  "description": "Manages Call log",
  "version": "1.0",

  "browser_action": {
    "default_icon": "logo.png",
     "homepage_url": "http://myoperator.co/",
    "default_popup": "popup.html"
  },
  "icons":{
    "64": "logo.png"
  },
  "permissions": [ 
    "activeTab",
    "http://newstageapi.voicetree.info/fetch/token",
    "storage" 
  ]
}